{
    "$id": "update",
    "type": "object",
    "required": [
        "uploadId",
        "username"
    ],
    "minProperties": 3,
    "properties": {
        "uploadId": {
            "$ref": "common#/definitions/uploadId"
        },
        "username": {
            "$ref": "common#/definitions/owner"
        },
        "directOnly": {
            "type": "boolean"
        },
        "meta": {
            "default": {},
            "allOf": [
                {
                    "not": {
                        "required": [
                            "export"
                        ]
                    }
                },
                {
                    "$ref": "common#/definitions/meta"
                }
            ]
        }
    }
}
