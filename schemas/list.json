{
    "$id": "list",
    "type": "object",
    "properties": {
        "offset": {
            "type": "integer",
            "minimum": 0,
            "default": 0
        },
        "limit": {
            "type": "integer",
            "default": 10,
            "minimum": 1,
            "maximum": 100
        },
        "order": {
            "type": "string",
            "enum": [
                "ASC",
                "DESC"
            ],
            "default": "DESC"
        },
        "criteria": {
            "type": "string"
        },
        "owner": {
            "$ref": "common#/definitions/owner"
        },
        "public": {
            "type": "boolean"
        },
        "temp": {
            "type": "boolean"
        },
        "expiration": {
            "type": "integer"
        },
        "without": {
            "type": "array",
            "minItems": 0,
            "items": {
                "type": "string",
                "minLength": 1
            }
        },
        "filter": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "uploadId": {
                    "$ref": "common#/definitions/filter-props"
                },
                "name": {
                    "$ref": "common#/definitions/filter-props"
                },
                "alias": {
                    "$ref": "common#/definitions/filter-props"
                },
                "description": {
                    "$ref": "common#/definitions/filter-props"
                },
                "website": {
                    "$ref": "common#/definitions/filter-props"
                },
                "owner": {
                    "$ref": "common#/definitions/filter-props"
                },
                "startedAt": {
                    "$ref": "common#/definitions/filter-props"
                },
                "uploadedAt": {
                    "$ref": "common#/definitions/filter-props"
                },
                "contentLength": {
                    "$ref": "common#/definitions/filter-props"
                },
                "status": {
                    "$ref": "common#/definitions/filter-props"
                },
                "parts": {
                    "$ref": "common#/definitions/filter-props"
                },
                "parentId": {
                    "$ref": "common#/definitions/filter-props"
                },
                "immutable": {
                    "$ref": "common#/definitions/filter-props"
                },
                "hasClones": {
                    "$ref": "common#/definitions/filter-props"
                },
                "isClone": {
                    "$ref": "common#/definitions/filter-props"
                },
                "clonedAt": {
                    "$ref": "common#/definitions/filter-props"
                },
                "nftOwner": {
                    "$ref": "common#/definitions/filter-props"
                },
                "nftToken": {
                    "$ref": "common#/definitions/filter-props"
                },
                "nftCollection": {
                    "$ref": "common#/definitions/filter-props"
                },
                "nftAmount": {
                    "$ref": "common#/definitions/filter-props"
                },
                "#": {
                    "$ref": "common#/definitions/filter-props"
                },
                "#multi": {
                    "type": "object",
                    "additionalProperties": false,
                    "required": [
                        "fields",
                        "match"
                    ],
                    "properties": {
                        "fields": {
                            "type": "array",
                            "minItems": 1,
                            "maxItems": 5,
                            "uniqueItems": true,
                            "items": {
                                "type": "string",
                                "enum": [
                                    "name",
                                    "description",
                                    "website",
                                    "owner",
                                    "alias"
                                ]
                            }
                        },
                        "match": {
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 50
                        }
                    }
                }
            }
        },
        "tags": {
            "type": "array",
            "minItems": 0,
            "uniqueItems": true,
            "items": {
                "type": "string",
                "minLength": 1,
                "maxLength": 50
            }
        },
        "modelType": {
            "type": "string",
            "enum": ["3d", "nft"]
        }
    }
}
