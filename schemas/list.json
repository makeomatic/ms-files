{
  "id": "list",
  "type": "object",
  "properties": {
    "offset": {
      "type": "integer",
      "minimum": 0,
      "default": 0
    },
    "limit": {
      "type": "integer",
      "default": 10,
      "minimum": 1,
      "maximum": 100
    },
    "order": {
      "type": "string",
      "enum": ["ASC", "DESC"],
      "default": "DESC"
    },
    "criteria": {
      "type": "string"
    },
    "owner": {
      "$ref": "common#/definitions/owner"
    },
    "public": {
      "type": "boolean"
    },
    "temp": {
      "type": "boolean"
    },
    "expiration": {
      "type": "integer"
    },
    "filter": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "uploadId": {
          "$ref": "common#/definitions/filter-props"
        },
        "name": {
          "$ref": "common#/definitions/filter-props"
        },
        "description": {
          "$ref": "common#/definitions/filter-props"
        },
        "website": {
          "$ref": "common#/definitions/filter-props"
        },
        "owner": {
          "$ref": "common#/definitions/filter-props"
        },
        "startedAt": {
          "$ref": "common#/definitions/filter-props"
        },
        "uploadedAt": {
          "$ref": "common#/definitions/filter-props"
        },
        "contentLength": {
          "$ref": "common#/definitions/filter-props"
        },
        "status": {
          "$ref": "common#/definitions/filter-props"
        },
        "parts": {
          "$ref": "common#/definitions/filter-props"
        },
        "#": {
          "$ref": "common#/definitions/filter-props"
        },
        "#multi": {
          "type": "object",
          "additionalProperties": false,
          "required": ["fields", "match"],
          "properties": {
            "fields": {
              "type": "array",
              "minItems": 1,
              "items": {
                "type": "string",
                "enum": ["name", "description", "website", "owner"]
              }
            },
            "match": {
              "type": "string",
              "minLength": 1,
              "maxLength": 50
            }
          }
        }
      }
    },
    "tags": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "string",
        "minLength": 1,
        "maxLength": 50
      }
    }
  }
}