{
  "name": "ms-files",
  "description": "Opinionated solution for managing files with GCS and other cloud storage providers",
  "main": "./lib/index.js",
  "version": "14.1.1",
  "scripts": {
    "compile": "rimraf ./lib && babel -d ./lib --copy-files ./src",
    "pretest": "yarn compile",
    "test": "rimraf ./coverage && yarn lint && yarn test:e2e",
    "test:e2e": "yarn test:e2e:cluster && yarn test:e2e:sentinel",
    "test:e2e:cluster": "mdep test run",
    "test:e2e:sentinel": "mdep test run --docker_compose ./test/docker-compose.sentinel.yml",
    "start": "mfleet",
    "lint": "eslint ./src",
    "prepublishOnly": "yarn compile",
    "semantic-release": "semantic-release",
    "docker-release": "mdep docker release",
    "release": "yarn --frozen-lockfile && yarn semantic-release"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/ms-files.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-files/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-files#readme",
  "dependencies": {
    "@google-cloud/storage": "3.0.3",
    "@hapi/hapi": "^18.1.0",
    "@microfleet/core": "^14.0.0",
    "@microfleet/transport-amqp": "^14.0.1",
    "@microfleet/validation": "^8.1.2",
    "@sentry/node": "^5.5.0",
    "bl": "^3.0.0",
    "bluebird": "^3.5.5",
    "common-errors": "^1.0.5",
    "dlock": "^9.0.0",
    "flatstr": "^1.0.12",
    "gcs-resumable-upload": "^2.2.0",
    "ioredis": "4.12.1",
    "is": "^3.3.0",
    "jaeger-client": "^3.15.0",
    "lodash": "^4.17.14",
    "md5": "^2.2.1",
    "mime-types": "^2.1.24",
    "moment": "^2.23.0",
    "ms-conf": "^5.0.0",
    "ms-files-transport": "^1.0.2",
    "ms-perf": "^1.0.1",
    "opentracing-js-ioredis": "2.1.2",
    "prom-client": "^11.5.3",
    "raven": "^2.6.4",
    "redis-filtered-sort": "^2.3.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.1",
    "stdout-stream": "^1.4.1",
    "uuid": "^3.3.2",
    "yargs": "^13.3.0"
  },
  "optionalDependencies": {
    "@google-cloud/pubsub": "^0.30.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-transform-strict-mode": "^7.2.0",
    "@babel/register": "^7.4.4",
    "@makeomatic/deploy": "^8.4.4",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/exec": "^3.3.5",
    "@semantic-release/git": "^7.0.16",
    "babel-eslint": "^10.0.2",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-makeomatic": "^3.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-promise": "^4.2.1",
    "faker": "^4.1.0",
    "json": "^9.0.6",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.1",
    "sinon": "^7.3.2"
  },
  "engines": {
    "node": ">= 10.15.0",
    "npm": ">= 4.0.0"
  },
  "files": [
    "src/",
    "schemas/",
    "lib/",
    "lua/",
    "bin/"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $HUSKY_GIT_PARAMS"
    }
  }
}
