{
  "name": "ms-files",
  "description": "Opinionated solution for managing files with GCS and other cloud storage providers",
  "main": "./lib/index.js",
  "version": "0.0.0-development",
  "scripts": {
    "compile": "babel -d ./lib --copy-files ./src",
    "pretest": "npm run compile",
    "test": "rimraf ./coverage && npm run lint && npm run test:e2e -- -r 'grpc --update-binary'",
    "test:e2e": "mdep test run",
    "start": "mfleet",
    "lint": "eslint ./src",
    "semantic-release": "semantic-release pre && npm run compile && npm publish && semantic-release post",
    "docker-release": "mdep docker release",
    "release": "yarn; npm run semantic-release; npm run docker-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/ms-files.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-files/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-files#readme",
  "dependencies": {
    "@google-cloud/storage": "^1.2.0",
    "@microfleet/core": "^9.3.0",
    "@microfleet/transport-amqp": "^10.1.0",
    "bl": "^1.2.1",
    "bluebird": "^3.5.0",
    "bunyan": "^1.8.10",
    "bunyan-noop": "^2.0.0",
    "bunyan-sentry-stream": "^1.1.0",
    "common-errors": "^1.0.2",
    "dlock": "^6.4.0",
    "gcs-resumable-upload": "^0.8.0",
    "ioredis": "^3.1.1",
    "is": "^3.2.1",
    "jaeger-client": "^3.5.3",
    "lodash": "^4.17.3",
    "md5": "^2.2.1",
    "mime-types": "^2.1.15",
    "moment": "^2.18.1",
    "ms-conf": "^3.1.2",
    "ms-files-transport": "^1.0.2",
    "ms-perf": "^1.0.1",
    "ms-validation": "^5.0.1",
    "opentracing-js-ioredis": "^1.0.0",
    "raven": "^2.1.0",
    "redis-filtered-sort": "^2.2.1",
    "spm-agent-nodejs": "^1.30.9",
    "stdout-stream": "^1.4.0",
    "uuid": "^3.1.0",
    "yargs": "^8.0.2"
  },
  "optionalDependencies": {
    "@google-cloud/pubsub": "^0.13.1"
  },
  "devDependencies": {
    "@makeomatic/deploy": "^1.12.2",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "codecov": "^2.2.0",
    "cross-env": "^5.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.2.0",
    "eslint-config-makeomatic": "^1.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^3.5.0",
    "faker": "^4.1.0",
    "json": "^9.0.6",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "rimraf": "^2.6.1",
    "semantic-release": "6.3.6",
    "sinon": "^2.3.8"
