{
  "name": "ms-files",
  "description": "Opinionated solution for managing files with GCS and other cloud storage providers",
  "main": "./lib/index.js",
  "version": "0.0.0-development",
  "scripts": {
    "compile": "babel -d ./lib --copy-files ./src",
    "pretest": "npm run compile",
    "test": "rimraf ./coverage && npm run lint && npm run test:e2e -- -r 'grpc --update-binary'",
    "test:e2e": "mdep test run",
    "start": "mfleet",
    "lint": "eslint .",
    "prepublishOnly": "npm run compile",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docker-release": "mdep docker release",
    "release": "yarn && npm run semantic-release && npm run docker-release",
    "commit": "simple-commit-message"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/ms-files.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-files/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-files#readme",
  "dependencies": {
    "@google-cloud/storage": "^1.2.1",
    "@microfleet/core": "^9.7.2",
    "@microfleet/transport-amqp": "^12.2.1",
    "bl": "^1.2.1",
    "bluebird": "^3.5.0",
    "bunyan": "^1.8.12",
    "bunyan-noop": "^2.0.0",
    "bunyan-sentry-stream": "^1.1.0",
    "common-errors": "^1.0.4",
    "dlock": "^6.4.0",
    "flatstr": "^1.0.5",
    "gcs-resumable-upload": "^0.8.1",
    "ioredis": "^3.1.4",
    "is": "^3.2.1",
    "jaeger-client": "^3.5.3",
    "lodash": "^4.17.3",
    "md5": "^2.2.1",
    "mime-types": "^2.1.16",
    "moment": "^2.18.1",
    "ms-conf": "^3.1.2",
    "ms-files-transport": "^1.0.2",
    "ms-perf": "^1.0.1",
    "ms-validation": "^6.0.2",
    "opentracing-js-ioredis": "^1.0.0",
    "raven": "^2.1.2",
    "redis-filtered-sort": "^2.2.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "spm-agent-nodejs": "^1.30.9",
    "stdout-stream": "^1.4.0",
    "uuid": "^3.1.0",
    "yargs": "^8.0.2"
  },
  "optionalDependencies": {
    "@google-cloud/pubsub": "^0.13.2"
  },
  "devDependencies": {
    "@makeomatic/deploy": "^4.1.4",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "codecov": "^2.3.0",
    "cross-env": "^5.0.5",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.5.0",
    "eslint-config-makeomatic": "^1.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^3.5.0",
    "faker": "^4.1.0",
    "json": "^9.0.6",
    "mocha": "^3.5.0",
    "nyc": "^11.0.3",
    "rimraf": "^2.6.1",
    "semantic-release": "^7.0.2",
    "sinon": "^3.2.1"
  },
  "release": {
    "verifyConditions": "@makeomatic/condition-semaphore",
    "generateNotes": "github-post-release",
    "analyzeCommits": "simple-commit-message",
    "branch": "master",
    "getLastRelease": "@makeomatic/last-release-npm"
  },
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [],
      "pre-push": [],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  },
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 3.0.0"
  }
}
