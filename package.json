{
  "name": "ms-files",
  "description": "Core of microservice",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "babel -d ./lib --copy-files ./src",
    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "test": "rimraf ./coverage && npm run lint && ./test/docker.sh",
    "start": "node ./bin/mservice.js | bunyan -o short",
    "lint": "eslint ./src",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/ms-files.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-files/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-files#readme",
  "dependencies": {
    "bluebird": "^3.5.0",
    "bunyan": "^1.8.8",
    "bunyan-sentry-stream": "^1.1.0",
    "common-errors": "^1.0.0",
    "dlock": "^6.3.0",
    "ioredis": "^2.4.3",
    "is": "^3.2.1",
    "lodash": "^4.17.3",
    "md5": "^2.2.1",
    "mime-types": "^2.1.14",
    "moment": "^2.17.1",
    "ms-amqp-transport": "^7.1.0",
    "ms-conf": "^2.3.0",
    "ms-files-gce": "^3.0.0",
    "ms-files-providers": "^2.0.2",
    "ms-perf": "^1.0.1",
    "ms-validation": "^3.0.0",
    "mservice": "^6.0.1",
    "raven": "^1.1.4",
    "redis-filtered-sort": "^2.0.6",
    "spm-agent-nodejs": "^1.30.7",
    "stdout-stream": "^1.4.0",
    "uuid": "^3.0.1",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-strict-mode": "^6.22.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "cross-env": "^3.2.3",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.17.1",
    "eslint-config-makeomatic": "^1.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "faker": "^4.1.0",
    "json": "^9.0.6",
    "latest-version-cli": "^1.1.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "request": "^2.81.0",
    "request-promise": "^4.1.1",
    "rimraf": "^2.6.1",
    "semantic-release": "6.3.6",
    "sinon": "^1.17.6"
  },
  "release": {
    "verifyConditions": []
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 3.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ]
  }
}
